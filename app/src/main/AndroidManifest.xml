<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lfaiska.mypomodoro">

    <application
        android:name="com.lfaiska.mypomodoro.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.lfaiska.mypomodoro.presenter.scenes.splash.view.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".presenter.scenes.home.view.HomeActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="608debf62cfb8ad49f202da41f493cb4a69461cf" />

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
